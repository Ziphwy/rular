{
  "name": "rular",
  "version": "0.2.0",
  "description": "A tool for front-end development, annotated design",
  "main": "index.js",
  "scripts": {
    "dev:server": "./node_modules/.bin/webpack-dev-server",
    "dev:electron": "cross-env NODE_ENV=development ./node_modules/.bin/electron ./src/index.js",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack -p --profile && ./node_modules/.bin/electron-builder --projectDir build"
  },
  "keywords": [],
  "author": "zfhuang",
  "license": "Apache-2.0",
  "dependencies": {
    "vue": "^2.5.2",
    "vuex": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ziphwy/rular.git"
  },
  "build": {
    "appId": "com.zfhuang.rular",
    "productName": "Rular",
    "asar": true,
    "asarUnpack": [
      "./assets/*"
    ],
    "compression": "normal",
    "electronVersion": "1.7.9",
    "mac": {
      "category": "tool",
      "icon": "icon/rular.icns"
    },
    "win": {
      "icon": "icon/rular.ico"
    }
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron-builder": "^19.43.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2"
  }
}
